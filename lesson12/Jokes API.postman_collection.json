		{
			"name": "get joke types",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Count of types should be grater then 0\", function () {",
							"    pm.response.to.have.status(200);",
							"    const response = pm.response.json();",
							"    pm.collectionVariables.unset(\"jokeTypes\");",
							"    pm.collectionVariables.set(\"jokeTypes\", JSON.stringify(response));",
							"    pm.expect(response).to.have.length.greaterThan(0);",
							"});",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{baseUrl}}/types",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"types"
					]
				}
			},
			"response": []
		},
		{
			"name": "get random joke",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Verify joke structure\", function () {",
							"    const randomJokeJson = pm.response.json();",
							"    pm.expect(randomJokeJson).to.not.be.empty;",
							"    pm.expect(randomJokeJson).to.have.keys(\"type\", \"setup\", \"punchline\", \"id\");",
							"});",
							"",
							"pm.test(\"Joke type should be one of expected\", function () {",
							"    const randomJokeJson = pm.response.json();",
							"    pm.collectionVariables.unset(\"joke\");",
							"    pm.collectionVariables.unset(\"jokeId\");",
							"    pm.collectionVariables.set(\"joke\", JSON.stringify(randomJokeJson));",
							"    pm.collectionVariables.set(\"jokeId\", randomJokeJson.id);",
							"    const jokeType = randomJokeJson.type;",
							"    const expectedTypes = JSON.parse(pm.collectionVariables.get(\"jokeTypes\"));",
							"    pm.expect(expectedTypes).to.include(jokeType);",
							"});",
							"",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{baseUrl}}/random_joke",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"random_joke"
					]
				}
			},
			"response": []
		},
